        "@commitlint/config-conventional": "^12.1.4",
        "@tsconfig/node12": "^1.0.7",
        "@types/jest": "^26.0.23",
        "@typescript-eslint/eslint-plugin": "^4.24.0",
        "@typescript-eslint/parser": "^4.24.0",
        "babel-jest": "^26.6.3",
        "eslint": "^7.26.0",
        "eslint-config-prettier": "^8.3.0",
        "husky": "^6.0.0",
